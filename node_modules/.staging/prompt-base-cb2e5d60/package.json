{
  "name": "prompt-base",
  "description": "Base prompt module used for creating custom prompt types for Enquirer.",
  "version": "0.6.2",
  "homepage": "https://github.com/enquirer/prompt-base",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)"
  ],
  "repository": "enquirer/prompt-base",
  "bugs": {
    "url": "https://github.com/enquirer/prompt-base/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "base": "^0.13.0",
    "debug": "^2.6.6",
    "define-property": "^1.0.0",
    "log-utils": "^0.2.1",
    "prompt-question": "^1.0.0",
    "readline-ui": "^0.1.3"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.12",
    "mocha": "^3.3.0",
    "repeat-string": "^1.6.1"
  },
  "keywords": [
    "answer",
    "answers",
    "ask",
    "bar",
    "base",
    "bottom",
    "checkbox",
    "choice",
    "cli",
    "command",
    "enquirer",
    "input",
    "inquire",
    "inquirer",
    "interact",
    "list",
    "menu",
    "password",
    "prompt",
    "prompts",
    "question",
    "readline",
    "stdin",
    "stdout",
    "terminal",
    "tty",
    "ui"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "enquirer",
        "prompt-choices",
        "prompt-question",
        "readline-utils"
      ],
      "prompts": [
        "prompt-autocomplete",
        "prompt-checkbox",
        "prompt-confirm",
        "prompt-editor",
        "prompt-expand",
        "prompt-list",
        "prompt-password",
        "prompt-radio",
        "prompt-rawlist"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "choices-separator",
      "enquirer",
      "prompt-question",
      "readline-ui"
    ]
  }
}
