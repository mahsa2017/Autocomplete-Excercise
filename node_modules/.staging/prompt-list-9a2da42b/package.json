{
  "name": "prompt-list",
  "description": "List-style prompt. Can be used as a standalone prompt, or with a prompt system like [Enquirer].",
  "version": "0.2.0",
  "homepage": "https://github.com/enquirer/prompt-list",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward <brian.woodward@gmail.com> (https://twitter.com/doowb)",
    "Jon Schlinkert <jon.schlinkert@sellside.com> (http://twitter.com/jonschlinkert)"
  ],
  "repository": "enquirer/prompt-list",
  "bugs": {
    "url": "https://github.com/enquirer/prompt-list/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "debug": "^2.6.3",
    "log-utils": "^0.2.1",
    "prompt-base": "^0.6.0",
    "terminal-paginator": "^0.2.1"
  },
  "devDependencies": {
    "enquirer": "^0.4.1",
    "gulp-format-md": "^0.1.12",
    "mocha": "^3.2.0"
  },
  "keywords": [
    "answer",
    "answers",
    "ask",
    "checkbox",
    "choice",
    "cli",
    "command",
    "enquirerprompt",
    "input",
    "inquire",
    "inquirer",
    "interact",
    "list",
    "menu",
    "password",
    "prompt",
    "prompts",
    "question",
    "readline",
    "stdin",
    "stdout",
    "terminal",
    "tty",
    "ui"
  ],
  "verb": {
    "toc": false,
    "layout": "common-minimal",
    "tasks": [
      "readme"
    ],
    "data": {
      "alias": "list"
    },
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "enquirer",
        "enquirer-prompt",
        "enquirer-question"
      ]
    },
    "reflinks": [
      "enquirer",
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
